{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Webpack Dev",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_PROJECT": "tsconfig-webpack.json"
      },
      "externalConsole": false,
      "sourceMaps": false,
      "outDir": null
    }
  ]
}
